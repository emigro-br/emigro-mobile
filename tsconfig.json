{
  "extends": "expo/tsconfig.base",
  "allowSyntheticDefaultImports": true,
  "jsx": "react-native",
  "lib": [
    "dom",
    "esnext"
  ],
  "include": [
    "./config/**/*",
    "./src/**/*",
    "./App.tsx"
  ],
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "moduleResolution": "node",
  "module": "commonjs",
  "noEmit": true,
  "target": "es6",
  "skipLibCheck": true,
  "resolveJsonModule": true,
  "strict": true,
  "baseUrl": ".",
  "compilerOptions": {
    "strict": true,
    "module": "es6",
    "types":["@testing-library/jest-native", "jest", "node"],
    "paths": {
      "test-utils": ["./src/__utils__/test-utils"],
      "@api/*": [
        "./src/api/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@config/*": [
        "./config/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  }
}

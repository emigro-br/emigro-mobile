{
  "name": "emigro-mobile",
  "description": "Mobile App for Customers.",
  "version": "1.8.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint src/",
    "format": "prettier --write src/ && eslint --fix src/",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test": "jest",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "/__fixtures__/",
      "__mocks__",
      "/__utils__/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/build/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|@sentry/.*|sentry-expo|native-base|react-native-svg|@gluestack-ui|@legendapp)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleDirectories": [
      "node_modules",
      "__utils__"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.52",
    "@gluestack-ui/config": "^1.1.18",
    "@gluestack-ui/themed": "^1.1.22",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@sentry/react-native": "~5.22.0",
    "amazon-cognito-identity-js": "^6.3.0",
    "axios": "^1.6.8",
    "expo": "~51.0.2",
    "expo-application": "~5.9.1",
    "expo-build-properties": "~0.12.1",
    "expo-camera": "~15.0.5",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.1",
    "expo-crypto": "~13.0.2",
    "expo-haptics": "~13.0.1",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.11",
    "expo-secure-store": "~13.0.1",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.11",
    "lucide-react-native": "^0.367.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "pix-utils": "^2.5.0",
    "promise": "^8.3.0",
    "react": "18.2.0",
    "react-hook-form": "^7.51.4",
    "react-native": "0.74.1",
    "react-native-currency-input": "^1.1.1",
    "react-native-dropdown-select-list": "^2.0.4",
    "react-native-element-dropdown": "^2.10.1",
    "react-native-geocoding": "^0.5.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.2",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^20.9.0",
    "@types/react": "~18.2.79",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9",
    "eslint-config-universe": "^12",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-prettier": "^5",
    "expo-dev-client": "~4.0.13",
    "husky": "9",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.1",
    "jest-mock-console": "^2.0.0",
    "lint-staged": "^15.2.2",
    "patch-package": "^8.0.0",
    "prettier": "^3",
    "react-native-dotenv": "^3.4.2",
    "react-test-renderer": "^18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
